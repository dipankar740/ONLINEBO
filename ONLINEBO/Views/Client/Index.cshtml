<script>
    $(document).ready(function () {

        $('#fAcHolderC').prop('checked', true);
        $("#fAcHolder").show();
        $('#jAcHolderC').prop('checked', true);
        $("#jAcHolder").show();
        $('#bankInfoC').prop('checked', true);
        $("#bankInfo").show();
        $('#aAcHolderC').prop('checked', true);
        $("#aAcHolder").show();
        $('#n1AcHolderC').prop('checked', true);
        $("#n1AcHolder").show();
        $('#n2AcHolderC').prop('checked', true);
        $("#n2AcHolder").show();
        $('#docC').prop('checked', true);
        $("#doc").show();

        $('#fAcHolderC').click(function (event) {
            if (this.checked) {
                $("#fAcHolder").show();
            } else {
                $("#fAcHolder").hide();
            }
        });

        $('#jAcHolderC').click(function (event) {
            if (this.checked) {
                $("#jAcHolder").show();
            } else {
                $("#jAcHolder").hide();
            }
        });

        $('#bankInfoC').click(function (event) {
            if (this.checked) {
                $("#bankInfo").show();
            } else {
                $("#bankInfo").hide();
            }
        });

        $('#aAcHolderC').click(function (event) {
            if (this.checked) {
                $("#aAcHolder").show();
            } else {
                $("#aAcHolder").hide();
            }
        });

        $('#n1AcHolderC').click(function (event) {
            if (this.checked) {
                $("#n1AcHolder").show();
            } else {
                $("#n1AcHolder").hide();
            }
        });


        $('#n2AcHolderC').click(function (event) {
            if (this.checked) {
                $("#n2AcHolder").show();
            } else {
                $("#n2AcHolder").hide();
            }
        });


        $('#docC').click(function (event) {
            if (this.checked) {
                $("#doc").show();
            } else {
                $("#doc").hide();
            }
        });

    });
</script>

